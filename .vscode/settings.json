    "vector": "cpp",
    "ostream": "cpp",
    "__bit_reference": "cpp",
    "__debug": "cpp",
    "__errc": "cpp",
    "__locale": "cpp",
    "__mutex_base": "cpp",
    "__node_handle": "cpp",
    "__threading_support": "cpp",
    "__verbose_abort": "cpp",
