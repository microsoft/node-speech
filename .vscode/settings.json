{
  "files.associations": {
    "iostream": "cpp",
    "iosfwd": "cpp",
    "__config": "cpp",
    "__hash_table": "cpp",
    "__split_buffer": "cpp",
    "__tree": "cpp",
    "array": "cpp",
    "bitset": "cpp",
    "deque": "cpp",
    "initializer_list": "cpp",
    "map": "cpp",
    "set": "cpp",
    "stack": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
        "**/*.d.ts": {
      "when": "$(basename).ts"
    },
        "**/*.js.map": true
    }
}