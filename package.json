{
  "name": "vscode-node-speech",
  "version": "1.0.0",
  "description": "Native bindings for Micrsooft Speech SDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-node-speech"
  },
  "homepage": "https://github.com/Microsoft/vscode-node-speech",
  "keywords": [
    "vscode"
  ],
  "license": "MIT",
  "author": {
    "name": "Microsoft Corporation"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "preinstall": "node preinstall.js",
    "postinstall": "tsc -p ./",
    "build": "node-gyp build && tsc -p ./",
    "compile": "tsc -p ./",
