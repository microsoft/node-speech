    "install": "node-gyp rebuild",
    "watch": "tsc -watch -p ./",
    "test": "jest"
  },
  "devDependencies": {
    "decompress": "^4.2.1",
    "got": "^11.8.6",
    "@types/node": "18.x",
    "@types/decompress": "^4.2.5",
    "typescript": "^5.2.x",
    "@types/jest": "^29.5.5",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "dotenv": "^16.3.1",
    "node-addon-api": "^7.0.0"
  },
  "jest": {
    "testMatch": [
      "**/*.test.js"
    ]
  }
}